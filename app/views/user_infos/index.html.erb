<div class="container">
<p id="notice"><%= notice %></p>

<h1>Listing User Infos</h1>

<table class= "table table-striped">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Phone number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_infos.each do |user_info| %>
      <tr>
        <td><%= user_info.first_name %></td>
        <td><%= user_info.last_name %></td>
        <td><%= user_info.email %></td>
        <td><%= user_info.phone_number %></td>
        <td><%= link_to 'Show', user_info ,class:"btn btn-primary" %></td>>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Back', atm_machine_path(session[:my_atm]),class:"btn btn-danger" %>
</div>
